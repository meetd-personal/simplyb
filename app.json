{
  "expo": {
    "name": "Simply Business Tracker",
    "slug": "simply-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#007AFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.meetcreations.simply",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "google-oauth",
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.1087977326078-ios123abc456def789ghi012jkl345mno"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#007AFF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.meetcreations.simply",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "scheme": "simply",
    "privacy": "public",
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 30000
    },
    "plugins": [
      "expo-apple-authentication",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "eb278866-c0dc-4cc3-aaad-ce7b93c6dd3e"
      }
    },
    "owner": "digrajkarmeet"
  }
}
